{
  "name": "website-noorse",
  "version": "1.0.5",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knoorsesv/noorse-site"
  },
  "license": "MIT",
  "author": "gvdp",
  "scripts": {
    "build": "gatsby build && cp _redirects public/_redirects",
    "build-storybook": "storybook build",
    "build:dev": "cross-env CONTENTFUL_ENV=staging gatsby build",
    "build:fake-vv": "cross-env CONTENTFUL_ENV=staging REAL_VV=false start-server-and-test start:fake-vv http://localhost:4000/.well-known/apollo/server-health 'gatsby build'",
    "build:prod": "cross-env CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false gatsby build",
    "deploy": "netlify deploy --dir public",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint-commit": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install",
    "release": "standard-version && git push --follow-tags",
    "serve": "gatsby serve",
    "start": "CONTENTFUL_ENV=staging gatsby develop",
    "start:fake-vv": "nodemon --watch fake-vv.js fake-vv.js",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false gatsby develop",
    "storybook": "storybook dev -p 6006",
    "test": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test'",
    "test:func": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:func'",
    "test:local": "yarn --cwd tests test:local",
    "test:update-ci": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:update'",
    "update-all": "yarn build:fake-vv && yarn test:update-ci && yarn test:update-local && git commit -am \"chore: update screenshots\" && git push"
  },
  "lint-staged": {
    "*.js,*.jsx": "eslint --cache --fix",
    "*": "prettier --write"
  },
  "dependencies": {
    "@babel/preset-env": "7.19.4",
    "@babel/preset-react": "7.18.6",
    "@fontsource/lato": "4.5.10",
    "@fontsource/mulish": "4.5.13",
    "@netlify/classnames-template-literals": "1.0.3",
    "@tailwindcss/typography": "0.5.7",
    "date-fns": "2.29.3",
    "gatsby": "4.24.5",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-postcss": "5.24.0",
    "gatsby-plugin-react-helmet": "5.24.0",
    "gatsby-plugin-sass": "5.24.0",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-plugin-typography": "4.24.0",
    "gatsby-source-contentful": "7.22.0",
    "gatsby-source-filesystem": "4.24.0",
    "gatsby-source-graphql": "4.24.0",
    "gatsby-transformer-sharp": "4.24.0",
    "marked": "4.1.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-lazyload": "3.2.0",
    "react-typography": "0.16.20",
    "sass": "1.55.0",
    "tailwindcss-elevation": "1.0.1",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@storybook/addon-a11y": "7.0.0-alpha.46",
    "@storybook/addon-essentials": "7.0.0-alpha.46",
    "@storybook/addon-interactions": "7.0.0-alpha.46",
    "@storybook/addon-links": "7.0.0-alpha.46",
    "@storybook/react": "7.0.0-alpha.46",
    "@storybook/react-vite": "7.0.0-alpha.46",
    "@storybook/testing-library": "0.0.13",
    "apollo-server": "3.10.3",
    "autoprefixer": "10.4.13",
    "cross-env": "7.0.3",
    "eslint": "8.26.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.10",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "nodemon": "2.0.20",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.3.0",
    "prettier-plugin-tailwindcss": "0.1.13",
    "standard-version": "9.3.2",
    "start-server-and-test": "1.14.0",
    "storybook": "7.0.0-alpha.46",
    "tailwindcss": "3.2.1"
  },
  "volta": {
    "node": "18.12.0",
    "yarn": "1.22.19"
  }
}
