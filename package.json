    "eslint-plugin-react": "7.37.5",
    "globals": "16.0.0",
    "husky": "9.0.11",
    "json-sort-cli": "4.0.9",
    "lint-staged": "15.2.11",
    "nodemon": "3.0.3",
    "postcss": "8.4.49",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-packagejson": "2.4.12",
    "prettier-plugin-tailwindcss": "0.5.13",
    "react-responsive": "10.0.1",
    "start-server-and-test": "2.0.11",
