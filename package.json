{
  "name": "website-noorse",
  "version": "2.0.0",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knoorsesv/noorse-site.git"
  },
  "license": "MIT",
  "author": {
    "name": "gvdp"
  },
  "scripts": {
    "build": "astro build && cp _redirects public/_redirects",
    "build-storybook": "storybook build",
    "build:dev": "cross-env CONTENTFUL_ENV=staging astro build",
    "build:prod": "cross-env CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro build",
    "deploy": "netlify deploy --dir public",
    "format": "prettier . --write --plugin prettier-plugin-astro --plugin prettier-plugin-tailwindcss",
    "lint": "concurrently --group --prefix-colors auto \"pnpm:lint:*\"",
    "lint:astro": "astro check",
    "lint:js": "eslint --cache .",
    "prepare": "husky",
    "start": "CONTENTFUL_ENV=staging astro dev",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro dev",
    "storybook": "storybook dev -p 6006",
    "test": "start-server-and-test serve 'pnpm --cwd tests test'",
    "test:func": "start-server-and-test serve 'pnpm --cwd tests test:func'",
    "test:local": "pnpm --cwd tests test:local",
    "test:update-ci": "start-server-and-test serve 'pnpm --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve 'pnpm --cwd tests test:update'",
    "test:visual-local": "start-server-and-test serve 'pnpm --cwd tests test:visual'",
    "chromatic": "chromatic --exit-zero-on-changes",
    "update-vv-data": "node fetch-vv.mjs"
  },
  "lint-staged": {
    "*.js,*.jsx": "eslint --cache --fix",
    "*.astro": "astro check",
    "*": "prettier --write"
  },
  "dependencies": {
    "@astrojs/check": "0.7.0",
    "@astrojs/react": "3.6.2",
    "@astrojs/tailwind": "5.1.1",
    "@fontsource/lato": "5.0.21",
    "@fontsource/mulish": "5.0.18",
    "@netlify/classnames-template-literals": "1.0.3",
    "@storybook/test": "8.3.4",
    "@tailwindcss/typography": "0.5.15",
    "@types/react": "18.3.2",
    "@types/react-dom": "18.3.0",
    "@vitejs/plugin-react": "4.3.2",
    "astro": "4.15.11",
    "astro-imagetools": "0.9.0",
    "contentful": "11.0.3",
    "date-fns": "4.1.0",
    "marked": "14.1.2",
    "prop-types": "15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-helmet": "6.1.0",
    "react-lazy-load": "4.0.1",
    "react-typography": "0.16.23",
    "tailwindcss-elevation": "2.0.0",
    "typescript": "5.4.5",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@storybook/addon-a11y": "8.3.4",
    "@storybook/addon-essentials": "8.3.4",
    "@storybook/addon-interactions": "8.3.4",
    "@storybook/addon-links": "8.3.4",
    "@storybook/react": "8.3.4",
    "@storybook/react-vite": "8.3.4",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "10.4.19",
    "chromatic": "11.0.0",
    "concurrently": "9.0.1",
    "cross-env": "7.0.3",
    "eslint": "9.11.0",
    "eslint-config-gvdp": "1.0.5",
    "eslint-plugin-astro": "1.2.4",
    "eslint-plugin-jsx-a11y": "6.10.0",
    "eslint-plugin-react": "7.37.1",
    "globals": "15.9.0",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nodemon": "3.0.3",
    "postcss": "8.4.38",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-packagejson": "2.4.12",
    "prettier-plugin-tailwindcss": "0.5.13",
    "start-server-and-test": "2.0.3",
    "storybook": "8.3.4",
    "tailwindcss": "3.4.13",
    "typescript-eslint": "8.8.0",
    "vite": "5.1.4"
  },
  "packageManager": "pnpm@8.15.3",
  "volta": {
    "node": "20.12.2",
    "pnpm": "8.15.3"
  },
