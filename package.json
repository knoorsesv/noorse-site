{
  "name": "website-noorse",
  "version": "1.0.5",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knoorsesv/noorse-site"
  },
  "license": "MIT",
  "author": "gvdp",
  "scripts": {
    "build": "gatsby build && cp _redirects public/_redirects",
    "build:fake-vv": "start-server-and-test start:fake-vv http://localhost:4000/.well-known/apollo/server-health 'gatsby build'",
    "deploy": "netlify deploy --dir public",
    "lint": "pretty-quick --staged",
    "lint-commit": "commitlint -E HUSKY_GIT_PARAMS",
    "release": "standard-version && git push --follow-tags",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "start:master": "CONTENTFUL_ENV=master gatsby develop",
    "start:fake-vv": "nodemon --watch fake-vv.js fake-vv.js",
    "test": "start-server-and-test serve http://localhost:9000 'npm run test --prefix tests'",
    "test:func": "start-server-and-test serve http://localhost:9000 'npm run test:func --prefix tests'",
    "test:update-ci": "start-server-and-test serve http://localhost:9000 'npm run test:ci-update --prefix tests'",
    "test:update-local": "start-server-and-test serve http://localhost:9000 'npm run test:update --prefix tests'"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.8",
    "@babel/preset-react": "^7.16.7",
    "@fortawesome/fontawesome-svg-core": "1.2.36",
    "@fortawesome/free-brands-svg-icons": "6.0.0",
    "@fortawesome/free-solid-svg-icons": "6.1.1",
    "@fortawesome/react-fontawesome": "0.1.16",
    "@netlify/classnames-template-literals": "^1.0.3",
    "date-fns": "2.28.0",
    "gatsby": "^4.9.1",
    "gatsby-plugin-fontawesome-css": "^1.2.0",
    "gatsby-plugin-image": "^2.9.0",
    "gatsby-plugin-postcss": "^5.10.0",
    "gatsby-plugin-react-helmet": "^5.9.0",
    "gatsby-plugin-sass": "^5.9.0",
    "gatsby-plugin-sharp": "^4.9.0",
    "gatsby-plugin-typography": "^4.9.0",
    "gatsby-source-contentful": "^7.7.1",
    "gatsby-source-filesystem": "^4.9.0",
    "gatsby-source-graphql": "^4.9.1",
    "gatsby-transformer-sharp": "^4.9.0",
    "prop-types": "15.8.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-lazyload": "3.2.0",
    "react-typography": "0.16.20",
    "sass": "^1.49.8",
    "tailwindcss-elevation": "1.0.1",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "apollo-server": "^3.6.3",
    "autoprefixer": "^10.4.2",
    "husky": "7.0.4",
    "nodemon": "^2.0.15",
    "postcss": "^8.4.5",
    "prettier": "2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "pretty-quick": "3.1.3",
    "standard-version": "9.3.2",
    "start-server-and-test": "^1.14.0",
    "tailwindcss": "^3.0.15"
  },
  "volta": {
    "node": "14.18.2",
    "npm": "6.14.15"
  }
}
