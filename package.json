{
  "name": "gatsby-site-noorse",
  "version": "0.1.87",
  "private": true,
  "description": "Noorse Website",
  "bugs": {
    "url": "https://github.com/gvdp/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gvdp/noorse-site"
  },
  "license": "MIT",
  "author": "gvdp",
  "scripts": {
    "backstop:approve": "backstop approve",
    "backstop:test": "node backstop_data/run.js",
    "build": "gatsby build",
    "build:css": "tailwindcss build src/styles/tailwind-import.css -o src/output.css",
    "cypress:docker": "docker run -it -v $PWD:/e2e -w /e2e cypress/included:5.3.0 --config-file cypress-components.json",
    "cypress:open": "DEBUG=*webpack*,cypress-react-unit-test cypress open",
    "cypress:test": "yarn build:css && cypress run --env failOnSnapshotDiff=false",
    "cypress:test-update": "yarn build:css && cypress run --env updateSnapshots=true",
    "develop": "gatsby develop",
    "release": "standard-version && git push --follow-tags",
    "serve": "gatsby serve"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "11.0.0",
    "@fortawesome/fontawesome-svg-core": "1.2.30",
    "@fortawesome/free-brands-svg-icons": "5.14.0",
    "@fortawesome/free-solid-svg-icons": "5.15.1",
    "@fortawesome/react-fontawesome": "0.1.11",
    "backstopjs": "^5.0.6",
    "cypress": "^5.2.0",
    "cypress-react-unit-test": "^4.15.0",
    "date-fns": "2.16.1",
    "gatsby": "2.24.76",
    "gatsby-image": "2.4.21",
    "gatsby-plugin-google-analytics": "^2.3.14",
    "gatsby-plugin-postcss": "2.3.0",
    "gatsby-plugin-react-helmet": "3.3.14",
    "gatsby-plugin-sass": "2.4.2",
    "gatsby-plugin-sharp": "2.6.18",
    "gatsby-plugin-typography": "2.5.13",
    "gatsby-plugin-use-query-params": "^1.0.1",
    "gatsby-source-contentful": "3.1.3",
    "gatsby-source-filesystem": "2.3.8",
    "gatsby-source-graphql": "2.7.4",
    "gatsby-transformer-sharp": "2.5.18",
    "husky": "4.3.0",
    "node-sass": "4.14.1",
    "prettier": "2.0.5",
    "prettier-plugin-packagejson": "^2.2.7",
    "pretty-quick": "3.1.0",
    "prop-types": "15.7.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-helmet": "6.1.0",
    "react-lazyload": "2.6.7",
    "react-masonry-css": "1.0.14",
    "react-typography": "0.16.19",
    "standard-version": "9.0.0",
    "tailwindcss": "1.9.6",
    "tailwindcss-elevation": "0.3.4",
    "typography": "0.16.19",
    "use-query-params": "^1.1.8"
  }
}
