{
  "name": "website-noorse",
  "version": "2.0.0",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knoorsesv/noorse-site.git"
  },
  "license": "MIT",
  "author": {
    "name": "gvdp"
  },
  "scripts": {
    "build": "astro build && cp _redirects public/_redirects",
    "build-storybook": "storybook build",
    "build:dev": "cross-env CONTENTFUL_ENV=staging astro build",
    "build:prod": "cross-env CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro build",
    "deploy": "netlify deploy --dir public",
    "format": "prettier . --write --plugin prettier-plugin-astro --plugin prettier-plugin-tailwindcss && jsonsort data",
    "lint": "concurrently --group --prefix-colors auto \"pnpm:lint:*\"",
    "lint:astro": "astro check",
    "lint:js": "eslint --cache .",
    "prepare": "husky",
    "start": "concurrently --group \"pnpm start:dev\" \"pnpm start:storybook\"",
    "start:dev": "CONTENTFUL_ENV=staging astro dev",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro dev",
    "start:storybook": "storybook dev -p 6006",
    "test": "start-server-and-test serve 'pnpm --cwd tests test'",
    "test:storybook": "test-storybook",
    "test:func": "start-server-and-test serve 'pnpm --cwd tests test:func'",
    "test:local": "pnpm --cwd tests test:local",
    "test:update-ci": "start-server-and-test serve 'pnpm --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve 'pnpm --cwd tests test:update'",
    "test:visual-local": "start-server-and-test serve 'pnpm --cwd tests test:visual'",
    "chromatic": "chromatic --exit-zero-on-changes",
    "update-vv-data": "node fetch-vv.mjs && pnpm format"
  },
  "lint-staged": {
    "*.js,*.jsx,*.tsx": "eslint --cache --fix",
    "*.astro": "astro check",
    "*": "prettier --write"
  },
  "dependencies": {
    "@astrojs/check": "0.9.5",
    "@astrojs/react": "4.4.0",
    "@fontsource/lato": "5.2.7",
    "@fontsource/mulish": "5.2.8",
    "@netlify/classnames-template-literals": "1.0.3",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.16",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@vitejs/plugin-react": "5.1.0",
    "astro": "5.15.2",
    "astro-imagetools": "0.9.0",
    "contentful": "11.8.7",
    "date-fns": "4.1.0",
    "marked": "16.4.1",
    "prop-types": "15.8.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-helmet": "6.1.0",
    "react-lazy-load": "4.0.1",
    "react-typography": "0.16.23",
    "tailwindcss-elevation": "2.0.0",
    "typescript": "5.9.3",
    "typography": "0.16.24"
  },
  "devDependencies": {
    "@commitlint/cli": "20.1.0",
    "@commitlint/config-conventional": "20.0.0",
    "@storybook/addon-a11y": "9.1.9",
    "@storybook/addon-links": "9.1.9",
    "@storybook/react-vite": "9.1.9",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "8.46.2",
    "autoprefixer": "10.4.21",
    "chromatic": "13.3.2",
    "concurrently": "9.2.1",
    "cross-env": "10.1.0",
    "eslint": "9.39.1",
    "eslint-config-gvdp": "1.0.8",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.4.0",
    "husky": "9.1.7",
    "json-sort-cli": "4.1.0",
    "lint-staged": "16.2.6",
    "nodemon": "3.1.10",
    "postcss": "8.5.6",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-packagejson": "2.5.19",
    "prettier-plugin-tailwindcss": "0.7.1",
    "react-responsive": "10.0.1",
    "start-server-and-test": "2.1.2",
    "storybook": "9.1.9",
    "tailwindcss": "4.1.16",
    "typescript-eslint": "8.46.2",
    "vite": "7.1.12",
    "eslint-plugin-storybook": "9.1.9",
    "@storybook/addon-docs": "9.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "volta": {
    "node": "22.21.1",
    "pnpm": "9.5.0"
  },
