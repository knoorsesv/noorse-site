{
  "name": "website-noorse",
  "version": "1.0.5",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knoorsesv/noorse-site"
  },
  "license": "MIT",
  "author": "gvdp",
  "scripts": {
    "build": "gatsby build && cp _redirects public/_redirects",
    "build:dev": "CONTENTFUL_ENV=staging gatsby build",
    "build:fake-vv": "cross-env CONTENTFUL_ENV=staging REAL_VV=false start-server-and-test start:fake-vv http://localhost:4000/.well-known/apollo/server-health 'gatsby build'",
    "deploy": "netlify deploy --dir public",
    "lint": "pretty-quick --staged",
    "lint-commit": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install",
    "release": "standard-version && git push --follow-tags",
    "serve": "gatsby serve",
    "start": "gatsby develop",
    "start:fake-vv": "nodemon --watch fake-vv.js fake-vv.js",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false gatsby develop",
    "test": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test'",
    "test:func": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:func'",
    "test:update-ci": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:update'"
  },
  "dependencies": {
    "@babel/preset-env": "7.19.0",
    "@babel/preset-react": "7.18.6",
    "@fontsource/lato": "4.5.9",
    "@fontsource/mulish": "4.5.12",
    "@netlify/classnames-template-literals": "1.0.3",
    "date-fns": "2.29.2",
    "gatsby": "4.22.0",
    "gatsby-plugin-image": "2.22.0",
    "gatsby-plugin-postcss": "5.22.0",
    "gatsby-plugin-react-helmet": "5.22.0",
    "gatsby-plugin-sass": "5.22.0",
    "gatsby-plugin-sharp": "4.22.0",
    "gatsby-plugin-typography": "4.22.0",
    "gatsby-source-contentful": "7.20.0",
    "gatsby-source-filesystem": "4.22.0",
    "gatsby-source-graphql": "4.22.0",
    "gatsby-transformer-sharp": "4.22.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-lazyload": "3.2.0",
    "react-typography": "0.16.20",
    "sass": "1.54.8",
    "tailwindcss-elevation": "1.0.1",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "apollo-server": "3.10.2",
    "autoprefixer": "10.4.8",
    "cross-env": "7.0.3",
    "husky": "8.0.1",
    "nodemon": "2.0.19",
    "postcss": "8.4.16",
    "prettier": "2.7.1",
    "prettier-plugin-packagejson": "2.2.18",
    "prettier-plugin-tailwindcss": "0.1.13",
    "pretty-quick": "3.1.3",
    "standard-version": "9.3.2",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "3.1.8"
  },
  "volta": {
    "node": "16.17.0",
    "yarn": "1.22.19"
  }
}
