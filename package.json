    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "8.17.0",
    "autoprefixer": "10.4.21",
    "chromatic": "11.20.2",
    "concurrently": "9.1.2",
    "cross-env": "7.0.3",
