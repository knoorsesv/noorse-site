{
  "name": "website-noorse",
  "version": "2.0.0",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/knoorsesv/noorse-site.git"
  },
  "license": "MIT",
  "author": {
    "name": "gvdp"
  },
  "scripts": {
    "build": "astro build && cp _redirects public/_redirects",
    "build-storybook": "storybook build",
    "build:dev": "cross-env CONTENTFUL_ENV=staging astro build",
    "build:prod": "cross-env CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro build",
    "deploy": "netlify deploy --dir public",
    "format": "prettier . --write --plugin prettier-plugin-astro --plugin prettier-plugin-tailwindcss",
    "lint": "concurrently --group --prefix-colors auto \"npm:lint:*\"",
    "lint:astro": "astro check",
    "lint:js": "eslint --ext .js,.jsx,.ts,.tsx,.astro,.mjs,.cjs .",
    "prepare": "husky",
    "serve": "astro preview",
    "start": "CONTENTFUL_ENV=staging astro dev",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false astro dev",
    "storybook": "storybook dev -p 6006",
    "test": "start-server-and-test serve 'pnpm --cwd tests test'",
    "test:func": "start-server-and-test serve 'pnpm --cwd tests test:func'",
    "test:local": "pnpm --cwd tests test:local",
    "test:update-ci": "start-server-and-test serve 'pnpm --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve 'pnpm --cwd tests test:update'",
    "test:visual-local": "start-server-and-test serve 'pnpm --cwd tests test:visual'",
    "chromatic": "chromatic --exit-zero-on-changes",
    "update-vv-data": "node fetch-vv.mjs"
  },
  "lint-staged": {
    "*.js,*.jsx": "eslint --cache --fix",
    "*.astro": "astro check",
    "*": "prettier --write"
  },
  "dependencies": {
    "@astrojs/check": "0.5.10",
    "@astrojs/react": "3.1.1",
    "@astrojs/tailwind": "5.1.0",
    "@fontsource/lato": "5.0.20",
    "@fontsource/mulish": "5.0.17",
    "@netlify/classnames-template-literals": "1.0.3",
    "@tailwindcss/typography": "0.5.10",
    "@types/react": "18.2.67",
    "@types/react-dom": "18.2.22",
    "astro": "4.5.16",
    "astro-imagetools": "0.9.0",
    "contentful": "10.8.4",
    "date-fns": "3.6.0",
    "marked": "12.0.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-lazy-load": "4.0.1",
    "react-typography": "0.16.23",
    "tailwindcss-elevation": "2.0.0",
    "typescript": "5.4.4",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "19.0.3",
    "@commitlint/config-conventional": "19.0.3",
    "@storybook/addon-a11y": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-interactions": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/jest": "0.2.3",
    "@storybook/react": "7.6.17",
    "@storybook/react-vite": "7.6.17",
    "@storybook/testing-library": "0.2.2",
    "@types/react-helmet": "6.1.11",
    "@typescript-eslint/parser": "7.0.2",
    "autoprefixer": "10.4.19",
    "chromatic": "11.0.0",
    "concurrently": "8.2.2",
    "cross-env": "7.0.3",
    "eslint": "8.56.0",
    "eslint-plugin-astro": "0.31.4",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "nodemon": "3.0.3",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-astro": "0.13.0",
    "prettier-plugin-packagejson": "2.4.12",
    "prettier-plugin-tailwindcss": "0.5.13",
    "start-server-and-test": "2.0.3",
    "storybook": "7.6.17",
    "tailwindcss": "3.4.1",
    "vite": "5.1.4"
  },
  "packageManager": "pnpm@8.15.3",
