{
  "name": "website-noorse",
  "version": "1.0.5",
  "private": true,
  "description": "Website voor voetbalploeg K. Noorse S.V.",
  "bugs": {
    "url": "https://github.com/knoorsesv/noorse-site/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knoorsesv/noorse-site"
  },
  "license": "MIT",
  "author": "gvdp",
  "scripts": {
    "build": "gatsby build && cp _redirects public/_redirects",
    "build-storybook": "storybook build",
    "build:dev": "cross-env CONTENTFUL_ENV=staging gatsby build",
    "build:fake-vv": "cross-env CONTENTFUL_ENV=staging REAL_VV=false start-server-and-test start:fake-vv http://localhost:4000/.well-known/apollo/server-health 'gatsby build'",
    "build:prod": "cross-env CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false gatsby build",
    "deploy": "netlify deploy --dir public",
    "format": "prettier . --write",
    "lint": "eslint .",
    "lint-commit": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install",
    "serve": "gatsby serve",
    "start": "CONTENTFUL_ENV=staging REAL_VV=false concurrently yarn:start:fake-vv yarn:start:app",
    "start:app": "CONTENTFUL_ENV=staging gatsby develop",
    "start:fake-vv": "nodemon --watch fake-vv.js fake-vv.js",
    "start:master": "CONTENTFUL_ENV=master CONTENTFUL_PREVIEW=false gatsby develop",
    "storybook": "storybook dev -p 6006",
    "test": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test'",
    "test:build-and-local": "yarn build:fake-vv && yarn test:visual-local",
    "test:func": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:func'",
    "test:local": "yarn --cwd tests test:local",
    "test:update-ci": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:ci-update'",
    "test:update-local": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:update'",
    "test:visual-local": "start-server-and-test serve http://localhost:9000 'yarn --cwd tests test:visual'"
  },
  "lint-staged": {
    "*.js,*.jsx": "eslint --cache --fix",
    "*": "prettier --write"
  },
  "dependencies": {
    "@babel/preset-env": "7.22.9",
    "@babel/preset-react": "7.22.5",
    "@fontsource/lato": "5.0.5",
    "@fontsource/mulish": "5.0.5",
    "@netlify/classnames-template-literals": "1.0.3",
    "@tailwindcss/typography": "0.5.9",
    "date-fns": "2.30.0",
    "gatsby": "4.24.8",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-postcss": "5.24.0",
    "gatsby-plugin-react-helmet": "5.24.0",
    "gatsby-plugin-sharp": "4.24.0",
    "gatsby-plugin-typography": "4.24.0",
    "gatsby-source-contentful": "7.22.0",
    "gatsby-source-filesystem": "4.24.0",
    "gatsby-source-graphql": "4.24.0",
    "gatsby-transformer-sharp": "4.24.0",
    "marked": "5.1.1",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-helmet": "6.1.0",
    "react-lazyload": "3.2.0",
    "react-typography": "0.16.23",
    "tailwindcss-elevation": "2.0.0",
    "typography": "0.16.21"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.6",
    "@commitlint/config-conventional": "17.6.6",
    "@storybook/addon-a11y": "7.0.0-beta.61",
    "@storybook/addon-essentials": "7.0.0-beta.61",
    "@storybook/addon-interactions": "7.0.0-beta.61",
    "@storybook/addon-links": "7.0.0-beta.61",
    "@storybook/jest": "0.1.0",
    "@storybook/react": "7.0.0-beta.61",
    "@storybook/react-vite": "7.0.0-beta.61",
    "@storybook/testing-library": "0.2.0",
    "apollo-server": "3.12.0",
    "autoprefixer": "10.4.14",
    "concurrently": "8.2.0",
    "cross-env": "7.0.3",
    "eslint": "8.45.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "nodemon": "3.0.1",
    "postcss": "8.4.26",
    "prettier": "3.0.0",
    "prettier-plugin-packagejson": "2.4.5",
    "prettier-plugin-tailwindcss": "0.4.0",
    "start-server-and-test": "2.0.0",
    "storybook": "7.0.0-beta.61",
    "tailwindcss": "3.3.3",
    "vite": "4.4.4"
  },
  "volta": {
    "node": "18.16.1",
    "yarn": "1.22.19"
  }
}
