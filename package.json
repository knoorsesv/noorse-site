    "marked": "15.0.11",
    "prop-types": "15.8.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-helmet": "6.1.0",
    "react-lazy-load": "4.0.1",
    "react-typography": "0.16.23",
    "tailwindcss-elevation": "2.0.0",
    "typescript": "5.8.3",
    "typography": "0.16.24"
  },
  "devDependencies": {
    "@commitlint/cli": "19.6.1",
