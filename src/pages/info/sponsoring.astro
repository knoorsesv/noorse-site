---
import { getEntries, getPage } from '../../data/contentful/get-entries'
import { mapSponsor } from '../../data/contentful/mapping'
import Layout from '../../layouts/Layout.astro'
import { SponsorPage } from '../../react/pages'

const sponsors = await getEntries('sponsor', mapSponsor)
const page = await getPage('Sponsoring')
---

<Layout title="Info - Bestuur" currentURL={Astro.url}>
  <SponsorPage
    sponsors={sponsors}
    description={page?.body}
    brochure={page?.attachment[0]}
  />
</Layout>
