{
  "ci": {
    "collect": {
      "staticDistDir": "public",
      "settings": {
        "numberOfRuns": 2
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    },
    "assert": {
      "categories:performance": ["warn", { "minScore": 0.9 }],
      "categories:performance": ["error", { "minScore": 0.8 }]
    }
  }
}
